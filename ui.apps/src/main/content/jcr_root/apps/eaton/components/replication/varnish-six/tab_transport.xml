<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Panel"
    title="Transport">
    <items jcr:primaryType="cq:WidgetCollection">
        <uri
            jcr:primaryType="cq:Widget"
            defaultValue="varnish://varnish-mgt-prod1.tcc.etn.com"
            fieldDescription="protocol &quot;varnish://&quot; required"
            fieldLabel="URI"
            name="./transportUri"
            readOnly="{Boolean}true"
            value="varnish://varnish-mgt-prod1.tcc.etn.com"
            xtype="textfield"/>
        <user
            jcr:primaryType="cq:Widget"
            fieldDescription="user name for the transport credentials"
            fieldLabel="User"
            name="./transportUser"
            xtype="textfield"/>
        <password
            jcr:primaryType="cq:Widget"
            fieldDescription="password for the transport credentials"
            fieldLabel="Password"
            name="./transportPassword"
            xtype="password"/>
    </items>
</jcr:root>
