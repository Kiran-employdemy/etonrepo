<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Interactive Room  Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <Commontab
                jcr:primaryType="nt:unstructured"
                jcr:title="General Properties"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <hostURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                defaultValue="http://dhryeahy6gbej.cloudfront.net/"
                                fieldDescription="Host URL where videos are hosted [Current : http://dhryeahy6gbej.cloudfront.net/]"
                                fieldLabel="Host Url for Videos"
                                name="./hostURL"/>
                            <videoExtension
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Extension for Video [e.g : .mp4]"
                                fieldLabel="Video Extension"
                                name="./videoExtension"/>
                            <room
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Interactive Room"
                                name="./room"
                                value="livingroom">
                                <items jcr:primaryType="nt:unstructured">
                                    <livingroom
                                        jcr:primaryType="nt:unstructured"
                                        text="Living Room"
                                        value="livingroom"/>
                                    <bedroom
                                        jcr:primaryType="nt:unstructured"
                                        text="Bedroom"
                                        value="bedroom"/>
                                    <garage
                                        jcr:primaryType="nt:unstructured"
                                        text="Garage"
                                        value="garage"/>
                                </items>
                            </room>
                            <carouselPrev
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="(e.g. Previous Arrows)."
                                fieldLabel="Label of Carousel Prev"
                                name="./carouselPrev"/>
                            <carouselNext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="(e.g. Next Arrows)."
                                fieldLabel="Label of Carousel Next"
                                name="./carouselNext"/>
                            <quitLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="(e.g. On / Off)"
                                fieldLabel="Label of Carousel Quit"
                                name="./quitLabel"/>
                            <Modaltext/>
                            <redirectlink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Redirect Link"
                                name="./redirectlink"/>
                        </items>
                    </columns>
                </items>
            </Commontab>
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Message 1"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                name="./title1"/>
                            <intro/>
                            <buttontext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CTA Button Text"
                                name="./buttontext1"/>
                            <buttonlink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="CTA Button Link"
                                name="./buttonlink1"
                                rootPath="/content/eaton"/>
                        </items>
                    </columns>
                </items>
            </tab1>
