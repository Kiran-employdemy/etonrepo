                                    width="{Long}300"
                                    xtype="textfield"/>
                            </items>
                        </fieldConfig>
                    </locationlov>
                    <ctalov
                        jcr:primaryType="cq:Widget"
                        fieldLabel="CTA LOV"
                        name="./ctaLov"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            border="{Boolean}true"
                            hideLabel="{Boolean}true"
                            layout="form"
                            padding="10px"
                            width="1000"
                            xtype="multi-field-panel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <text
                                    jcr:primaryType="cq:Widget"
