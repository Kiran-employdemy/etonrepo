                                    dName="value"
                                    fieldLabel="Value"
                                    width="{Long}300"
                                    xtype="textfield"/>
                            </items>
                        </fieldConfig>
                    </businessgrplov>
                    <assetlov
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Asset LOV"
                        name="./assetLov"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
