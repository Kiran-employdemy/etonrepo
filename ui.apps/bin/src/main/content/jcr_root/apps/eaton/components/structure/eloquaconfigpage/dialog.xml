                                    dName="text"
                                    fieldLabel="Text"
                                    width="{Long}300"
                                    xtype="textfield"/>
                                <value
                                    jcr:primaryType="cq:Widget"
                                    dName="value"
                                    fieldLabel="Value"
                                    width="{Long}300"
                                    xtype="textfield"/>
                            </items>
                        </fieldConfig>
                    </stagelov>
                    <productlov
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Product LOV"
                        name="./productLov"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
