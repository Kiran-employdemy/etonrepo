                                    width="{Long}300"
                                    xtype="textfield"/>
                            </items>
                        </fieldConfig>
                    </productlov>
                    <businessgrplov
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Business Group LOV"
                        name="./businessGrpLov"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            border="{Boolean}true"
                            hideLabel="{Boolean}true"
                            layout="form"
                            padding="10px"
                            width="1000"
                            xtype="multi-field-panel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <text
                                    jcr:primaryType="cq:Widget"
                                    dName="text"
                                    fieldLabel="Text"
                                    width="{Long}300"
                                    xtype="textfield"/>
                                <value
