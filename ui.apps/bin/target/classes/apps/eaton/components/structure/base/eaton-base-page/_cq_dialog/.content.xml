<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,cq.siteadmin.admin.properties,cq.wcm.msm.properties,granite.contexthub.configuration]"
    mode="edit">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="cq-dialog-content-page">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                granite:class="cq-siteadmin-admin-properties-tabs"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true"
                size="L"
                rel="cq-siteadmin-admin-properties-tabs">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/apps/eaton/components/structure/base/eaton-base-page/tabs/basic-tab"/>
                    <advanced
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/apps/eaton/components/structure/base/eaton-base-page/tabs/advanced-tab"/>
                    <thumbnail
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/mnt/overlay/wcm/foundation/components/basicpage/v1/basicpage/tabs/thumbnail"/>
                    <cloudservices
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/mnt/overlay/wcm/foundation/components/basicpage/v1/basicpage/tabs/cloudservices"/>
                    <personalization
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/mnt/overlay/wcm/foundation/components/basicpage/v1/basicpage/tabs/personalization"/>
                    <permissions
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/mnt/overlay/wcm/core/content/sites/properties/tabs/permissions"/>
                    <blueprint
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/apps/eaton/components/structure/base/eaton-base-page/tabs/blueprint-tab"/>
                    <livecopy
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/apps/eaton/components/structure/base/eaton-base-page/tabs/livecopy-tab"/>
                    <inheritance
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/apps/eaton/components/structure/base/eaton-base-page/tabs/inheritance-tab"/>
                    <teaser
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/apps/eaton/components/structure/base/eaton-base-page/tabs/teaser"/>
                    <secure
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/apps/eaton/components/structure/base/eaton-base-page/tabs/secure"/>
					<softwaredelivery
							jcr:primaryType="nt:unstructured"
							sling:resourceType="granite/ui/components/coral/foundation/include"
							path="/apps/eaton/components/structure/base/eaton-base-page/tabs/software-delivery"/>
                    <support
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/apps/eaton/components/structure/base/eaton-base-page/tabs/support"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>