        "page_overview": {
          "jcr:primaryType": "nt:unstructured",
          "jcr:mixinTypes": [
            "cq:LiveRelationship"
          ],
          "jcr:createdBy": "e0456505",
          "jcr:lastModifiedBy": "e0456505",
          "cq:lastRolledout": "Mon Nov 04 2019 13:37:37 GMT-0500",
          "jcr:created": "Wed Jul 17 2019 13:52:33 GMT-0400",
          "pageOverviewHeaderType": "h2",
          "jcr:lastModified": "Mon Nov 04 2019 13:37:37 GMT-0500",
          "pageOverviewHeader": "Application and engineering expertise",
          "sling:resourceType": "eaton/components/content/page-overview",
          "textIsRich": "true",
          "pageOverviewDesc": "We understand what it takes to do everything from standard, fast cylinder repairs to turnkey projects with maintenance downtimes. Eaton uses world-class simulation tools, comprehensive 3D drawing capabilities and finite element analysis expertise for failure mode determination, prediction of future failure analysis and recommendation of general product improvements for optimum performance and reliability. Decades of application experience coupled with these cutting edge tools translates to better recommendations and, ultimately, better cylinder repairs.<br>\r\n",
          "cq:lastRolledoutBy": "c3049643"
        },
        "feature_block": {
          "jcr:primaryType": "nt:unstructured",
          "jcr:mixinTypes": [
            "cq:LiveRelationship"
          ],
          "jcr:createdBy": "e0456505",
          "featureHeader": "Systems and processes",
          "mobileTrans": "No Image Transformation - mobile",
          "jcr:lastModifiedBy": "e0456505",
          "cq:lastRolledout": "Mon Nov 04 2019 13:37:37 GMT-0500",
          "featureDesc": "Quality is an area in which we excel, and it's demonstrated every day in the new hydraulic cylinders we manufacture. Besides the facility ISO certifications, we provide comprehensive quality reviews and customer reporting documentation. We’ve set the industry standard in detailed inspection reports, documentation logs and customer communication processes, saving you significant time and money. ",
          "jcr:created": "Wed Jul 17 2019 13:47:33 GMT-0400",
          "featureAlignmentMobile": "center-center",
          "featureImageAltText": "Hydraulic cylinder repair",
          "featureAlignmentDesktop": "center-center",
          "featureImageAlignment": "left",
          "jcr:lastModified": "Mon Nov 04 2019 13:37:37 GMT-0500",
          "desktopTrans": "No Image Transformation - desktop",
          "featureImage": "/content/dam/eaton/services/images/hydraulic-cylinder-repair.jpg",
          "sling:resourceType": "eaton/components/content/feature-block",
          "cq:lastRolledoutBy": "c3049643",
          "featureBlockColor": "theme-light-gray"
        },
        "feature_block_937509669": {
          "jcr:primaryType": "nt:unstructured",
          "jcr:mixinTypes": [
            "cq:LiveRelationship"
          ],
          "jcr:createdBy": "e0456505",
          "featureHeader": "Welding and machining specialists",
          "mobileTrans": "No Image Transformation - mobile",
          "jcr:lastModifiedBy": "e0456505",
          "cq:lastRolledout": "Mon Nov 04 2019 13:37:37 GMT-0500",
          "featureDesc": "The integration of cylinder repair and service capability with new product manufacturing translates to better repairs for your product. Eaton’s extensive capabilities allow us to not only repair your hydraulic cylinders quickly, but to recommend structural improvements and provide improved coating technology and access to the latest seal technology. Our industry-leading equipment and employees ensure that you receive top-notch results every time.",
          "jcr:created": "Wed Jul 17 2019 13:48:52 GMT-0400",
          "featureAlignmentMobile": "center-center",
          "featureImageAltText": "eatonite",
          "featureAlignmentDesktop": "center-center",
          "featureImageAlignment": "right",
          "jcr:lastModified": "Mon Nov 04 2019 13:37:37 GMT-0500",
          "desktopTrans": "No Image Transformation - desktop",
          "featureImage": "/content/dam/eaton/markets/oil-and-gas/eatonite.jpg",
          "sling:resourceType": "eaton/components/content/feature-block",
          "cq:lastRolledoutBy": "c3049643",
          "featureBlockColor": "primary-branding-color"
        },
        "feature_block_1299358197": {
          "jcr:primaryType": "nt:unstructured",
          "jcr:mixinTypes": [
            "cq:LiveRelationship"
          ],
          "jcr:createdBy": "e0456505",
          "featureHeader": "Global footprint, local service",
          "mobileTrans": "No Image Transformation - mobile",
          "jcr:lastModifiedBy": "e0456505",
          "cq:lastRolledout": "Mon Nov 04 2019 13:37:37 GMT-0500",
          "featureDesc": "Eaton’s cylinder repair expertise covers everything from standard tie-rod cylinders to mill duty cylinders to extra large custom cylinders that span to bore sizes of 60 in (1500 mm) and strokes of 72 ft (22 m). With decades of experience in cylinder repair, we understand the urgency in critical situations, so you always get the response you need, when you need it. Our experienced staff can offer on-site advice, or pick up your cylinders, repair them and quickly return them to you. Our global footprint ensures that service is quick, close and convenient regardless of your project's location.",
          "jcr:created": "Wed Jul 17 2019 13:50:31 GMT-0400",
          "featureAlignmentMobile": "center-center",
          "featureImageAltText": "blue globe",
          "featureAlignmentDesktop": "center-center",
          "featureImageAlignment": "left",
          "jcr:lastModified": "Mon Nov 04 2019 13:37:37 GMT-0500",
          "desktopTrans": "No Image Transformation - desktop",
          "featureImage": "/content/dam/eaton/products/electrical-circuit-protection/bussmann-iec-high-speed-semi-conductors-fuses/Eaton Bussmann globe earth_FB.jpg",
          "sling:resourceType": "eaton/components/content/feature-block",
          "cq:lastRolledoutBy": "c3049643",
          "featureBlockColor": "theme-light-gray"
        },
